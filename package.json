{
  "name": "timelapseapp-ui",
  "version": "1.0.0",
  "description": "React frontend for the club-manager suite",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-relay": "relay-compiler --src src --schema schema.json",
    "watch-relay": "relay-compiler --src src --schema schema.json --watch",
    "start-js": "node scripts/start.js",
    "start": "npm-run-all -p watch-relay watch-css start-js",
    "build": "yarn build-css && yarn build-relay && node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "start:server": "node server"
  },
  "keywords": [],
  "author": "Stefan Foulis",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "autoprefixer": "7.1.0",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-relay": "^1.0.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "graphql": "^0.9.6",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.3",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "react-dev-utils": "^2.0.1",
    "react-error-overlay": "^1.0.6",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "2.6.0",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "dependencies": {
    "bootstrap": "next",
    "date-fns": "^1.28.5",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^15.5.4",
    "react-relay": "^1.1.0",
    "react-router-dom": "^4.1.1",
    "reactstrap": "^4.8.0",
    "relay-compiler": "^1.1.0",
    "relay-query-lookup-renderer": "^1.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "relay"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
